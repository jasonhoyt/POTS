<%= link_to image_tag("back.png"), vendors_path %>

<p id="notice"><%= notice %></p>

<p>
  <b>Vendor Company:</b><br/>
  <%= @vendor.company_name %><br/>
  <%= @vendor.address1 %><br/>
  <%= @vendor.address2 %><br/>
  <%= @vendor.city %>, <%= @vendor.state %> <%= @vendor.zip %>
</p>
<p>
  <b>Phone:</b> <%= @vendor.company_phone %><br/>
  <b>Fax:</b> <%= @vendor.company_fax %>
</p>
<p>
  <b>Website:</b>
  <%= @vendor.website %>
</p>
<p>
  <b>Contact:</b>
  <%= @vendor.contact_name %><br/>
  <%= @vendor.contact_phone %><br/>
  <%= @vendor.contact_email %>
</p>

<p>
  <b>Product lines:</b>
  <%= @vendor.tags %>
</p>

<p>
  <b>Note:</b>
  <%= @vendor.note %>
</p>

<%= link_to image_tag("back.png"), vendors_path %> |
<%= link_to image_tag("edit.png"), edit_vendor_path(@vendor) %> |
<%= link_to image_tag("delete.png"), @vendor, :confirm => 'Are you sure?', :method => :delete %>

